{
  "name": "nativeuikitten",
  "version": "0.0.1",
  "private": true,
  "description": "Projeto base react native utilizano uikitten, redux e reduxsaga",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "release": "react-native run-android --variant=release",
    "debug-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug && cd ..",
    "release-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable- && rm -rf android/app/src/main/res/raw/ && cd android && ./gradlew assembleRelease && cd ..",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@eva-design/eva": "2.0.0",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/cli-platform-android": "^2.9.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.2",
    "@react-navigation/bottom-tabs": "^5.5.2",
    "@react-navigation/native": "^5.5.1",
    "@react-navigation/stack": "^5.5.1",
    "@ui-kitten/components": "5.0.0",
    "@ui-kitten/eva-icons": "5.0.0",
    "axios": "^0.19.1",
    "immer": "^9.0.6",
    "react": "16.11.0",
    "react-content-loader": "^5.0.1",
    "react-native": "0.62.2",
    "react-native-awesome-alerts": "^1.3.0",
    "react-native-camera": "^3.19.1",
    "react-native-device-info": "^5.5.1",
    "react-native-fingerprint-scanner": "^4.0.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-permissions": "^2.0.9",
    "react-native-rename": "^2.4.1",
    "react-native-safe-area-context": "^3.0.2",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "^2.9.0",
    "react-native-svg": "^9.13.6",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/runtime": "^7.10.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "@ui-kitten/metro-config": "5.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.1.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^26.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
